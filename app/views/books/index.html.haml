-#  This file is app/views/books/index.html.haml
%h1 All Books

%table#books
  %thead
    %tr
      %th Book Title
      %th Author
      %th ISBN
      %th Quality
      %th Price
  %tbody
    - @books.each do |book|
      %tr
        %td= book.title 
        %td= book.author
        %td= book.isbn
        %td= book.quality
        %td= book.price

= link_to 'My Books', mybooks_path
